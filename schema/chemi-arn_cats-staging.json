{"openapi": "3.0.3", "info": {"title": "ARN Cats API", "version": "1.0", "description": "API allows generating a set of ARN categories via GET (one chemical) or POST (multiple chemicals)"}, "paths": {"/api/arn_cats/version": {"get": {"operationId": "app.get_version", "responses": {"200": {"description": "Success"}}}}, "/api/arn_cats/metadata": {"get": {"operationId": "app.get_metadata", "responses": {"200": {"description": "Success"}}}}, "/api/arn_cats": {"get": {"operationId": "app.get_cats", "summary": "Generate descriptors for one molecule", "parameters": [{"name": "smiles", "in": "query", "description": "SMILES to generate groups for", "required": true, "schema": {"type": "string"}, "examples": {"existing_ARNs": {"summary": "SMILES that generates one of the 56 ARN groups", "value": "CC(C)c1ccc2sc3ccccc3c(=O)c2c1"}, "other_ARNs": {"summary": "Miscellaneous chemistry", "value": "c1ccc(OP(Oc2ccccc2)Oc2ccccc2)cc1"}}}, {"name": "model", "in": "query", "description": "Model to use for group prediction", "required": false, "schema": {"type": "string", "default": "RF"}, "examples": {"default_model": {"summary": "Default RF prediction model", "value": "RF"}, "alternative_model": {"summary": "Alternative DNN-based prediction model", "value": "NN"}}}], "responses": {"200": {"description": "Returns a calculated group or error"}}}, "post": {"operationId": "app.post_cats", "summary": "Generate groups for multiple molecules", "requestBody": {"content": {"application/json": {"schema": {"type": "object", "properties": {"chemicals": {"type": "array"}, "model": {"type": "string", "enum": ["RF", "NN"], "default": "RF"}}, "required": ["chemicals"], "example": {"chemicals": ["FC(C(C(C1OC(=O)C2C(=CC=CC=2)N=1)(F)F)(F)F)(F)F", "CCCCN"], "model": "RF"}}}}}, "responses": {"200": {"description": "Returns calculated descriptors"}}}}}, "components": {"schemas": {"Request": {"type": "object", "properties": {"chemicals": {"type": "array"}, "model": {"type": "string", "enum": ["RF", "NN"], "default": "RF"}}, "required": ["chemicals"], "example": {"chemicals": ["FC(C(C(C1OC(=O)C2C(=CC=CC=2)N=1)(F)F)(F)F)(F)F", "CCCCN"], "model": "RF"}}}}, "servers": [{"url": ""}]}

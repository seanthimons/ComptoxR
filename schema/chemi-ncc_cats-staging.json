{"openapi": "3.0.3", "info": {"title": "NCC Cats API", "version": "1.0", "description": "API allows generating a set of NCC categories via GET (one chemical) or POST (multiple chemicals)"}, "paths": {"/api/ncc_cats/version": {"get": {"operationId": "app.get_version", "responses": {"200": {"description": "Success"}}}}, "/api/ncc_cats/metadata": {"get": {"operationId": "app.get_metadata", "responses": {"200": {"description": "Success"}}}}, "/api/ncc_cats": {"get": {"operationId": "app.get_cats", "summary": "Generate categories for one molecule", "parameters": [{"name": "smiles", "in": "query", "description": "SMILES string of the molecule", "required": true, "schema": {"type": "string"}, "example": "C1=CC=CC=C1C(C1C=CC=CC=1)C1C=CC=CC=1"}, {"name": "logp", "in": "query", "description": "Octanol-water partition coefficient", "required": true, "schema": {"type": "number", "format": "float"}, "example": 5.76}, {"name": "ws", "in": "query", "description": "Water solubility (mg/L)", "required": true, "schema": {"type": "number", "format": "float"}, "example": 4.0738}], "responses": {"200": {"description": "Returns a category of molecule or error"}}}, "post": {"operationId": "app.post_cats", "summary": "Generate categories for multiple molecules", "requestBody": {"content": {"application/json": {"schema": {"type": "array", "items": {"type": "object", "properties": {"smiles": {"type": "string"}, "logp": {"type": "number", "format": "float"}, "ws": {"type": "number", "format": "float"}}, "required": ["smiles", "logp", "ws"], "example": [{"smiles": "C1=CC=CC=C1C(C1C=CC=CC=1)C1C=CC=CC=1", "logp": 5.76, "ws": 4.0738e-07}, {"smiles": "ICCCI", "logp": 3.02, "ws": 0.0007413}]}}}}}, "responses": {"200": {"description": "Returns categories"}}}}}, "components": {"schemas": {"Request": {"type": "array", "items": {"type": "object", "properties": {"smiles": {"type": "string"}, "logp": {"type": "number", "format": "float"}, "ws": {"type": "number", "format": "float"}}, "required": ["smiles", "logp", "ws"], "example": [{"smiles": "C1=CC=CC=C1C(C1C=CC=CC=1)C1C=CC=CC=1", "logp": 5.76, "ws": 4.0738e-07}, {"smiles": "ICCCI", "logp": 3.02, "ws": 0.0007413}]}}, "Chemical": {"type": "object", "properties": {"smiles": {"type": "string"}, "logp": {"type": "number", "format": "float"}, "ws": {"type": "number", "format": "float"}}, "required": ["smiles", "logp", "ws"], "example": [{"smiles": "C1=CC=CC=C1C(C1C=CC=CC=1)C1C=CC=CC=1", "logp": 5.76, "ws": 4.0738e-07}, {"smiles": "ICCCI", "logp": 3.02, "ws": 0.0007413}]}}}, "servers": [{"url": ""}]}

{"swagger": "2.0", "info": {"title": "RDKit descriptors generator API", "version": "1.0", "description": "API allows generating a set of RDKit descriptors via GET (one chemical) or POST (multiple chemicals)"}, "consumes": ["application/json"], "produces": ["application/json"], "paths": {"/api/rdkit/version": {"get": {"operationId": "app.get_version", "responses": {"200": {"description": "Success"}}}}, "/api/rdkit/metadata": {"get": {"operationId": "app.get_metadata", "responses": {"200": {"description": "Success"}}}}, "/api/rdkit": {"get": {"operationId": "app.get_descr", "summary": "Generate descriptors for one molecule", "parameters": [{"name": "smiles", "in": "query", "type": "string", "required": true}, {"name": "type", "in": "query", "type": "string", "required": false, "default": "ecfp"}, {"name": "radius", "in": "query", "type": "integer", "required": false, "default": 3}, {"name": "bits", "in": "query", "type": "integer", "required": false, "default": 1024}], "responses": {"200": {"description": "Returns calculated descriptors"}}}, "post": {"operationId": "app.post_descr", "summary": "Generate descriptors for multiple molecules", "parameters": [{"name": "request", "in": "body", "schema": {"type": "object", "required": ["chemicals"], "properties": {"chemicals": {"type": "array"}, "options": {"type": "object"}}}}], "responses": {"200": {"description": "Returns calculated descriptors"}}}}}, "definitions": {"Request": {"type": "object", "required": ["chemicals"], "properties": {"chemicals": {"type": "array"}, "options": {"type": "object"}}}}, "parameters": {}, "responses": {}, "basePath": ""}
